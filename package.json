{
  "name": "iframe-consent",
  "private": true,
  "version": "0.1.0",
  "description": "A web component which provides drop-in replacement for the html iframe tag",
  "main": "iframe-consent.js",
  "module": "iframe-consent.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:prod": "MODE=prod rollup -c",
    "clean": "rimraf iframe-consent.{d.ts,d.ts.map,js,js.map} test/iframe-consent.{d.ts,d.ts.map,js,js.map} test/iframe-consent_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat build/iframe-consent.bundled.js | gzip -9 | wc -c ;"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "shry",
  "license": "",
  "dependencies": {
    "lit": "^2.4.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-legacy": "^1.0.1",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "eslint": "^8.26.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "~4.8.4"
  },
  "customElements": "custom-elements.json"
}
